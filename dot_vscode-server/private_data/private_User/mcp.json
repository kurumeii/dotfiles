{
	"servers": {
		"serena": {
			"httpUrl": "http://localhost:12345/mcp",
		}
    "context-7": {
			"type": "stdio",
			"command": "pnpm",
			"args": [
				"dlx",
				"@upstash/context7-mcp"
			],
			"env": {
				"api-key": "${secrets.CONTEXT_7_API_KEY}"
			}
		},
		"figma": {
			"type": "stdio",
			"command": "pnpm",
			"args": [
				"dlx",
				"figma-developer-mcp",
				"--stdio"
			],
			"env": {
				"FIGMA_API_KEY": "${secrets.FIGMA_API_KEY}"
			}
		}
	},
	"inputs": []
}
