{
	"servers": {
		"serena": {
			"url": "http://localhost:12345/mcp",
			"type": "http"
		},
		"context-7": {
			"type": "stdio",
			"command": "pnpm",
			"args": [
				"dlx",
				"@upstash/context7-mcp"
			],
			"env": {
				"api-key": "$CONTEXT_7_API_KEY"
			}
		},
		"figma": {
			"type": "stdio",
			"command": "pnpm",
			"args": [
				"dlx",
				"figma-developer-mcp",
				"--stdio"
			],
			"env": {
				"FIGMA_API_KEY": "$FIGMA_API_KEY"
			}
		}
	},
	"inputs": []
}
